</body><script>var version=0;
var number=ZPeople.getCookie("People_banner"); // No I18N
version = '1'; //NO OUTPUTENCODING
  
if(version != null && version != undefined && version !=""){
if(number == version){
$("#peoplebanner").hide();
$("#wrapper").removeClass('tban');
}else if(number != version){
$("#peoplebanner").show();
$("#wrapper").addClass('tban');
}
}
</script></html>
